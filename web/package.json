{
  "name": "geist-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn sass && eleventy",
    "watch": "yarn sass:watch && eleventy --watch",
    "serve": "yarn assets && eleventy --serve",
    "dev": "yarn serve",
    "debug": "DEBUG=* eleventy",
    "sass": "node-sass -r resources/scss/styles.scss -wo _assets/css  --output-style compressed",
    "sass:watch": "yarn sass -w",
    "assets": "mkdir -p _site/ && cp -R _assets _site/"
  },
  "keywords": [],
  "author": "Jamie Bradley",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "node-sass": "^4.13.0",
    "sass-mq": "^5.0.1"
  }
}
